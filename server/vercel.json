{
    "version":2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "configs":{
                "includeFiles":[
                    "dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]   
}